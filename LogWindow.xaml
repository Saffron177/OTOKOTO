<Window x:Class="HottoMotto.LogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HottoMotto"
        mc:Ignorable="d"
        Title="LogWindow" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="LogListBox" HorizontalAlignment="Left" Height="414" Margin="251,0,0,0" VerticalAlignment="Center" Width="539">

        </ListBox>
        <ListBox x:Name="logList" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="10,49,564,10"/>
        <TextBox x:Name="search_Textbox" HorizontalAlignment="Left" Margin="30,22,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="206" TextChanged="Search_Textbox_Changed"/>

    </Grid>
</Window>
